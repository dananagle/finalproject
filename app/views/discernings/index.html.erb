<p id="notice"><%= notice %></p>

<h1>Impressions</h1>

<div class="table-responsive">
  <table class="table">
    <thead>
    <tr>
      <th class="rotate"><div><span>Walk</span></div></th>
      <th class="rotate"><div><span>Presence</span></div></th>
      <th class="rotate"><div><span>Personality</span></div></th>
      <th class="rotate"><div><span>Posture</span></div></th>
      <th class="rotate"><div><span>Carriage</span></div></th>
      <th class="rotate"><div><span>Bone Structure</span></div></th>
      <th class="rotate"><div><span>Smile</span></div></th>
      <th class="rotate"><div><span>Overall Appearance</span></div></th>
      <th class="rotate"><div><span>Je ne sais quoi</span></div></th>
      <th class="rotate"><div><span>Distinctiveness</span></div></th>



      <th>Comment</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @discernings.each do |discerning| %>
      <tr>
        <td><%= discerning.walk %></td>
        <td><%= discerning.presence %></td>
        <td><%= discerning.personality %></td>
        <td><%= discerning.posture %></td>
        <td><%= discerning.carriage %></td>
        <td><%= discerning.bone %></td>
        <td><%= discerning.smile %></td>
        <td><%= discerning.appearance %></td>
        <td><%= discerning.distinctiveness %></td>
        <td><%= discerning.jenesaisquoi %></td>
        <td><%= discerning.comment %></td>
        <td><%= link_to 'Show', discerning %></td>
        <td><%= link_to 'Edit', edit_discerning_path(discerning) %></td>
        <td><%= link_to 'Destroy', discerning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>

</div><!-- /.table-responsive -->
<br>

<%= link_to 'New Impressions', new_discerning_path %>
